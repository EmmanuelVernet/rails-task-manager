<div class="container">
<h1>Show all tasks</h1>
  <ul>
    <% @tasks.each do |task|%>
      <br>
      <li>Task: <%= link_to task.title, task_path(task) %></li>
      <li> Task details: <%= link_to task.details, task_path(task) %></li>
      <%# <li><%= link_to task.completed, show_task_path(task) %>
    <% end %>
  </ul>
  <%= link_to "New task", new_task_path %>
</div>